	  <UseWindowsForms>true</UseWindowsForms>

	  <!-- Datos para el paquete-->
	  <PackageVersion>1.0.0.1</PackageVersion>
	  <PackageOutputPath>../Package/</PackageOutputPath>
	  <Version>1.0.0.1</Version>
	  <Authors>Kraker</Authors>
